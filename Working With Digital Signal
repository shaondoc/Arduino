/*
Working With Digital Signal 
github: http://https://github.com/shaondoc
https://www.tinkercad.com/things/d1mIKpKONhR
*/
void setup()
{
  pinMode(13, OUTPUT); 
  pinMode(10, INPUT);
}

void loop()
{
  int val=digitalRead(10);
  if(val==HIGH){
    digitalWrite(13, HIGH);
  }
  else{
    digitalWrite(13, LOW);
  }
  
}
